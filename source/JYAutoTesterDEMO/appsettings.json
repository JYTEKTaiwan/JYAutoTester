{
  "MATSys": {
    "Modules": [
      {
        "Type": "Tester",
        "Alias": "Tester",
        "Recorder": {
          "Type": "text"
        }
      },
      {
        "Type": "Handler",
        "Alias": "Handler"
        
      },
      {
        "Type": "TestReporter",
        "Alias": "TestReporter",
        "Logfolder": ""
      }
    
    ],
    "ScriptMode": true,
    "Scripts": {
      "Setup": [ "{ \"Script\": \"setup.ats\"}" ],
      "Test": [
        "{ \"Executer\": {\"Handler\":{\"Moving\":[]}}}",
        "{ \"Script\": \"main.ats\"}"
      ],
      "TearDown": [ "{ \"Script\": \"teardown.ats\"}" ]
    },
    "References": {
      "Recorders": [ ".\\modules\\TextRecorder.dll" ]
    }
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "async": true,
      "logfile": {
        "type": "File",
        "fileName": "./${shortdate}.log"
      },
      "logconsole": {
        "type": "Console"
      }
    },
    "rules": [
      {
        "logger": "none",
        "minLevel": "Trace",
        "writeTo": "logfile"
      }
    ]
  }
}
